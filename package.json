{
  "name": "bridge-to-europe",
  "author": "Mutasem Halawani",
  "version": "1.0.0",
  "description": "Bridge to Europe - NLI",
  "main": "index.js",
  "scripts": {
    "prestart": "npm rebuild node-sass",
    "start": "webpack-dev-server --inline --content-base dist/ --open",
    "eslint": "./node_modules/.bin/eslint src/*.js",
    "accessibility": "node ./dev/accessibility-validation.js",
    "htmllint": "node ./dev/html-validator.js",
    "sasslint": "node ./node_modules/sass-lint/bin/sass-lint.js -v -q",
    "test": "npm run buildapp && karma start && npm run eslint && npm run accessibility && npm run htmllint && npm run sasslint",
    "build": "webpack -p",
    "start:dev": "webpack-dev-server",
    "sassdoc": "node ./dev/sassdoc.js",
    "docsserver": "npm run sassdoc && webpack-dev-server --port 8010 --config webpack.docs.config.js --content-base ./docs/ --open"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "access-sniff": "^3.1.0",
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.8",
    "cssnano": "^3.10.0",
    "ejs-render-loader": "^1.0.0",
    "eslint": "^4.15.0",
    "extract-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "filehound": "^1.16.2",
    "html-loader": "^0.5.4",
    "html-validator": "^2.2.3",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "sassdoc": "^2.5.0",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.10.1"
  }
}
